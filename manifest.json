{
  "name": "LinkeTheme",
  "description": "An eye-catching cleaner theme extension for LinkedIn",
  "version": "1.0.0",
  "homepage_url": "https://github.com/daltonmenezes/linketheme",
  "manifest_version": 2,
  "content_scripts": [{
    "matches": ["*://www.linkedin.com/*"],
    "css": ["css/theme.css"],
    "run_at": "document_start"
  }],
  "permissions": [
    "*://www.linkedin.com/*"
  ],
  "developer": {
    "name": "daltonmenezes"
  },
  "icons": {
    "48": "icons/48.png", 
    "96": "icons/96.png"
  },
  "page_action": {
    "default_icon": {
      "48": "icons/48.png", 
      "96": "icons/96.png"
    }
  }  
}